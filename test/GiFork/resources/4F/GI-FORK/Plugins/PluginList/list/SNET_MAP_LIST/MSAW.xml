	<col key="col8" indata="@SNET_LIST(FIELD_TO_CHECK1=..inhibition, VAL2=hMax)@" outdata="UPPER INHIB" col_width="110"  />
	<col key="col9" indata="" outdata="EXC" row_image="@SNET_LIST(FIELD_TO_CHECK=..EX_CRI, INFO_TYPE=IMAGE)@" row_leftClick="SHOW_CONTAINER_IN_GRP(VALUE=MSAW%20EXCLUSION%20CRITERIA%20@KEY_ID@)"  />
