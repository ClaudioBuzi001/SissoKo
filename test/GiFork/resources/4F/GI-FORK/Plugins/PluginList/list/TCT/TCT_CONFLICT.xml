        <col key="col14" indata="@ALIAS_LIST(FIELD_TO_CHECK1=..COO2)@" outdata="COO2"   row_leftClick="SHOW_TCT_CONFLICT(CONFLICT_ID=@CONFLICT_NUMBER@)"/>
        <col key="col15" indata="@ALIAS_LIST(FIELD_TO_CHECK1=..EFX1)@" outdata="EFX1"   row_leftClick="SHOW_TCT_CONFLICT(CONFLICT_ID=@CONFLICT_NUMBER@)"/>
