<msg>
	<list 
		ROW_ID="@CONFLICT_NUMBER@" condition="@ISTCT@">
		<attributes 
			ID="TCT CONFLICT LIST" 
			BEHAVIOR_IF_EMPTY="delete" 
			MAX_ITEM_SHOWED="10"
			SORT="URG:ASC,TIME:ASC," 
			SIDE="ALR:1"
			 
			 
		/>
		<col key="col1"  indata="@CONFLICT_NUMBER@"	outdata="CONFLICT_ID"   row_leftClick="SHOW_TCT_CONFLICT(CONFLICT_ID=@CONFLICT_NUMBER@)"/>		
		<col key="col2"  indata="@ALIAS_LIST(FIELD_TO_CHECK1=..TIME_TO_CONFLICT)@" outdata="TIME"	  row_leftClick="SHOW_TCT_CONFLICT(CONFLICT_ID=@CONFLICT_NUMBER@)"/>		
		<col key="col3"  indata="@ALARM_TYPE@"	outdata="TYPE"	row_fgr="@ALIAS_LIST(FIELD_TO_CHECK1=..URGENCY , INFO_TYPE=COLOR)@"  row_leftClick="SHOW_TCT_CONFLICT(CONFLICT_ID=@CONFLICT_NUMBER@)" />
		<!-- col key="col4"  indata="@URGENCY@"	         outdata="URG"	   row_leftClick="SHOW_TCT_CONFLICT(CONFLICT_ID=@CONFLICT_NUMBER@)" /-->
		<col key="col4"  indata="@ALIAS_LIST(FIELD_TO_CHECK1=..CS_RCS_SSR_1)@" outdata="CS1"  row_fgr="@LIST_FGR_COLOR(FIELD_TO_CHECK=..TEMPLATE , VALUE_TO_CHECK = TRACK_NUMBER1, DEFAULT=#fafafa)@" row_blink_state="@ALIAS_LIST(FIELD_TO_CHECK=..TEMPLATE, INFO_TYPE=BLINK)@" row_leftPress="HIGHLIGHT_TCT_CONFLICT(HIGHLIGHT=TRUE, TRK_ID=LBL_@TRACK_NUMBER1@)" row_leftRelease="HIGHLIGHT_TCT_CONFLICT(HIGHLIGHT=FALSE, TRK_ID=LBL_@TRACK_NUMBER1@)"/>
		<col key="col5"  indata="@ALIAS_LIST(FIELD_TO_CHECK1=..CS_RCS_SSR_2)@" outdata="CS2"  row_fgr="@LIST_FGR_COLOR(FIELD_TO_CHECK=..TEMPLATE , VALUE_TO_CHECK = TRACK_NUMBER2, DEFAULT=#fafafa)@" row_blink_state="@ALIAS_LIST(FIELD_TO_CHECK=..TEMPLATE, INFO_TYPE=BLINK)@" row_leftPress="HIGHLIGHT_TCT_CONFLICT(HIGHLIGHT=TRUE, TRK_ID=LBL_@TRACK_NUMBER2@)" row_leftRelease="HIGHLIGHT_TCT_CONFLICT(HIGHLIGHT=FALSE, TRK_ID=LBL_@TRACK_NUMBER2@)" />
