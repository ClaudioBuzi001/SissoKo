		<col key="col9" indata="@ALIAS_LIST(FIELD_TO_CHECK1=..PRESENT_PLANAR_DISTANCE)@"	 outdata="DIST"   row_leftClick="SHOW_TCT_CONFLICT(CONFLICT_ID=@CONFLICT_NUMBER@)" />
		<col key="col10" indata="@ALIAS_LIST(FIELD_TO_CHECK1=..MINIMUM_ABS_DISTANCE)@"	 outdata="MINABSDIST"   row_leftClick="SHOW_TCT_CONFLICT(CONFLICT_ID=@CONFLICT_NUMBER@)" />		
		<col key="col11" indata="@ALIAS_LIST(FIELD_TO_CHECK1=..EST_DISTANCE_TO_SEP_LOSS_1)@" outdata="DFI1"   row_leftClick="SHOW_TCT_CONFLICT(CONFLICT_ID=@CONFLICT_NUMBER@)"/>
