	<col key="col4" indata="@CPDLC_ALIAS(FIELD_TO_CHECK1=..answer, VAL_TO_CHECK=classeId)@"  outdata="RESPONSE" col_state="filteredVisible" />
	<col key="col5" indata="@ALIAS_LIST(FIELD_TO_CHECK=..response)@" outdata="RD"  col_state="filteredVisible" row_fgr="@CPDLC_ALIAS(FIELD_TO_CHECK1=..response, VAL_TO_CHECK=classeId, DEFAULT=#FFFFFF, INFO_TYPE=COLOR)@" row_leftClick="@CPDLC_ALIAS(FIELD_TO_CHECK1=..CALLBACK_RD, VAL_TO_CHECK=classeId)@" />
	
