<msg>

<!-- CONFIGURATA LIST PER SETTORE SE AIRPORT CONTROLLATO-->
<list ROW_ID="@FLIGHT_NUM@" condition="@ISDEP_SECT@">

<attributes ID="DEP:@ADEP@"
	BEHAVIOR_IF_EMPTY="delete"
	GROUP="DEP" SIDE="SFL:2" SORT="ETD:ASC" multicolumnname="ADEP" />

<col key="col1"  indata="@TACSTATUS@"  outdata="TCS"      />
<col key="col2"	 indata="@CALLSIGN@"   outdata="CALLSIGN" row_fgr="@TEMPLATE_COLOR?TEMPLATE_COLOR;#FFFFFF@" row_middleClick="CONTEXT_MENU(MENU_FILE=Tower_MENU.xml, TITLE=TOWER_MENU)" row_leftClick="CONTEXT_MENU(MENU_FILE=Ground_MENU.xml,TITLE=GROUND_MENU)" row_rightClick="CONTEXT_MENU(MENU_FILE=Flight_MENU.xml,TITLE=FLIGHT_MENU)"/>
<col key="col3"  indata="@OBT@"        outdata="EOBT"     col_state="filteredNotVisible" />
<col key="col4"  indata="@ETD@"        outdata="ETD" />
<col key="col5"  indata="@CTOT@"       outdata="CTOT"     col_state="filteredVisible"  row_leftClick="EXTERNAL_ORDER(GRAPHIC_ORDER=PREVIEW ,ORDER_ID=CTOT, OBJECT_TYPE=CTOT, HOST_NAME=BCV, PREVIEW_NAME=previewCTOT)" />
<col key="col6"  indata="@TOBT@"       outdata="TOBT"     col_state="filteredVisible"/>
<col key="col7"  indata="@ATY@ "       outdata="TYPE"     col_state="filteredVisible"  />
<col key="col8"  indata="@WTC@"        outdata="W"        col_state="filteredVisible"  />
<col key="col9"	 indata="@DEP_PKBAY@"  outdata="PKB"	  col_state="filteredVisible"  row_leftClick="EXTERNAL_ORDER(GRAPHIC_ORDER=PREVIEW ,ORDER_ID=EPKBDEP, OBJECT_TYPE=EPKBDEP, HOST_NAME=BCV, PREVIEW_NAME=previewEPKBDEP)" />
<col key="col10" indata="@DEPRWY@"     outdata="RWY"      row_leftClick="EXTERNAL_ORDER(GRAPHIC_ORDER=PREVIEW ,ORDER_ID=RWADEP, OBJECT_TYPE=RWADEP, HOST_NAME=BCV, PREVIEW_NAME=choiceRWADEP)"/>
<col key="col11" indata="@SID@"        outdata="SID    "  row_leftClick="EXTERNAL_ORDER(GRAPHIC_ORDER=PREVIEW ,ORDER_ID=CSI, OBJECT_TYPE=CSI, HOST_NAME=BCV, PREVIEW_NAME=previewCSI)" />
<col key="col12" indata="@FULLROUTE@"  outdata="ROUTE"    col_state="filteredVisible"/>
<col key="col13" indata="@ADES@"       outdata="ADES"     col_state="filteredVisible"  />

</list>


</msg>
